<!-- body -->
<div class="container w-75" style="min-height: 72vh; margin-top: 30px">
    <div class="row">
        <div class="container">
            <h2>List shared generated data</h2>
            <div id="accordion">
                {{#each remapGeneratedFiles}}
                <div class="card">
                    <div class="card-header">
                        <a class="" data-bs-toggle="collapse" href="#collapse{{this.id}}">
                            <p>{{this.fileName}}</p>
                        </a>
                    </div>
                    <div id="collapse{{this.id}}" class="collapse" data-bs-parent="#accordion">
                        <div class="card-body">
                            <h3>Content file</h3>
                            <pre>{{this.content}}</pre>
                            <button type="button" onclick="window.location.href='{{this.link}}'"
                                class="btn btn-primary">Download</button>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
    <div class="col-sm-6">
    </div>
</div>